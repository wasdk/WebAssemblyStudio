{
  "name": "web-assembly-studio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build-watch": "webpack --watch --config webpack.config.js",
    "dev-server": "webpack-dev-server --config webpack.config.js",
    "test": "./node_modules/.bin/jest",
    "clean": "rm -r dist/*",
    "tslint": "./node_modules/.bin/tslint -p ."
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/tests/**/**/**.(ts|tsx|js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbebenita/WebAssemblyStudio.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mbebenita/WebAssemblyStudio/issues"
  },
  "homepage": "https://github.com/mbebenita/WebAssemblyStudio",
  "dependencies": {
    "@types/minimatch": "^3.0.2",
    "@types/mousetrap": "^1.5.34",
    "@types/react-icons": "^2.2.4",
    "@types/react-modal": "^3.1.1",
    "@types/xterm": "^2.0.3",
    "cassowary": "0.0.2",
    "minimatch": "^3.0.4",
    "monaco-editor": "^0.10.1",
    "mousetrap": "^1.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "react-modal": "^3.1.10",
    "wasmparser": "^0.5.3"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.8",
    "@types/jest": "^22.1.1",
    "@types/react": "^16.0.28",
    "@types/react-dom": "^16.0.3",
    "awesome-typescript-loader": "^3.4.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.1.4",
    "react-test-renderer": "^16.2.0",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
  }
}
